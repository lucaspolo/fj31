package br.com.caelum.loja.client.exemplo;

import java.util.List;

import br.com.caelum.loja.entity.Livro;
import br.com.caelum.loja.ws.ConsultaDeLivros;
import br.com.caelum.loja.ws.ConsultaDeLivrosBeanService;

public class ClienteAcessandoWebServiceLoja {
	public static void main(String[] args) {
		ConsultaDeLivros consulta = (ConsultaDeLivros) new ConsultaDeLivrosBeanService().getConsultaDeLivrosBeanPort();
		List<Livro> buscaLivro = consulta.buscaLivro("a");
	}
}
